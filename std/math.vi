in factorial inline
    # n => n! #
    
    dup
    dup

    let temp
    set temp

    temp 3 < if 
        pop pop
        temp

        0 ret
    end

    while
        1 - temp * set temp 1 - dup
        
        dup 2 = if break end
    end

    pop pop
    temp

    0 ret
end


in sqrt inline 
    # n => n^2 #

    dup *
    0 ret
end


in cube inline
    # n => n^3 #

    dup dup * *
    0 ret
end